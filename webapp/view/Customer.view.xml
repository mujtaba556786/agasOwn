<mvc:View xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:tnt="sap.tnt"
	xmlns:f="sap.f" controllerName="ag.agasown.controller.Customer">
	<Page id="productDetail" showNavButton="true" navButtonPress="onNavBack">
		<customHeader>
			<core:Fragment fragmentName="ag.agasown.view.fragment.Header" type="XML" />
		</customHeader>
		<content>
			<HBox class="sapUiLargeMargin" justifyContent="SpaceBetween">
				<VBox class="sideNav">
					<Text class="sideNavText" text="Your Account"></Text>
					<Button class="sideNavBtn" text="Overview" id="overview" press="onCustomerNavigationSelect" type="Unstyled"></Button>
					<Button class="sideNavBtn" text="Order" id="order" press="onCustomerNavigationSelect" type="Unstyled"></Button>
					<Button class="sideNavBtn" text="Newsletter" id="newsletter" press="onCustomerNavigationSelect" type="Unstyled"></Button>
					<Button class="sideNavBtn" text="Vouchers" id="vouchers" press="onCustomerNavigationSelect" type="Unstyled"></Button>
					<Button class="sideNavBtn" text="Wishlist" id="wishlist" press="onCustomerNavigationSelect" type="Unstyled"></Button>
					<Button class="sideNavBtn" text="Personal Details" id="personalDetails" press="onCustomerNavigationSelect" type="Unstyled"></Button>
					<Text class="sideNavText" text="More"></Text>
					<Button class="sideNavBtn" text="Help FAQ" id="helpFaq" press="onCustomerNavigationSelect" type="Unstyled"></Button>
					<Button class="sideNavBtn" text="Request and Delete" id="reqDel" press="onCustomerNavigationSelect" type="Unstyled"></Button>
					<Button class="sideNavBtn" text="ContactUs" id="contactUs" press="onCustomerNavigationSelect" type="Unstyled"></Button>
				</VBox>
				<VBox id="customerContent">

				</VBox>
			</HBox>

		</content>
		<footer>

		</footer>
	</Page>
</mvc:View>