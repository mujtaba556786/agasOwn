<mvc:View xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:tnt="sap.tnt"
	xmlns:f="sap.f" controllerName="ag.agasown.controller.Customer"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Page id="productDetail" showNavButton="true" navButtonPress="onNavBack">
		<customHeader>
			<core:Fragment fragmentName="ag.agasown.view.fragment.Header" type="XML" />
		</customHeader>
		<content>
			<HBox class="sapUiLargeMargin">
				<VBox class="sideNav">
					<Text class="sideNavText" text="Your Account"></Text>
					<Button class="sideNavBtn" app:fragmentName="Overview" text="Overview" id="overview" press="onCustomerNavigationSelect" type="Unstyled"></Button>
					<Button class="sideNavBtn" app:fragmentName="Order" text="Order" id="order" press="onCustomerNavigationSelect" type="Unstyled"></Button>
					<Button class="sideNavBtn" app:fragmentName="Wishlist" text="Wishlist" id="wishlist" press="onCustomerNavigationSelect" type="Unstyled"></Button>
					<Button class="sideNavBtn" app:fragmentName="Vouchers" text="Vouchers" id="vouchers" press="onCustomerNavigationSelect" type="Unstyled"></Button>
					<Button class="sideNavBtn" app:fragmentName="PersonalDetails" text="Personal Details" id="personalDetails" press="onCustomerNavigationSelect" type="Unstyled"></Button>
					<Text class="sideNavText" text="More"></Text>
					<Button class="sideNavBtn" app:fragmentName="HelpFaq" text="Help FAQ" id="helpFaq" press="onCustomerNavigationSelect" type="Unstyled"></Button>
					<Button class="sideNavBtn" app:fragmentName="ContactUs" text="ContactUs" id="contactUs" press="onCustomerNavigationSelect" type="Unstyled"></Button>
				</VBox>
				<Panel id="customerContent">

				</Panel>
			</HBox>
		</content>
		<footer>

		</footer>
	</Page>
</mvc:View>