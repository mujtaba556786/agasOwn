<mvc:View xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:tnt="sap.tnt"
	xmlns:f="sap.f" controllerName="ag.agasown.controller.Customer"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Page id="productDetail" showNavButton="true" navButtonPress="onNavBack">
		<customHeader>
			<core:Fragment fragmentName="ag.agasown.view.fragment.Header" type="XML" />
		</customHeader>
		<content>
			<HBox class="sapUiLargeMargin">
				<VBox class="sideNav">
					<Text class="sideNavText" text="{i18n>yourAcc}"></Text>
					<Button class="sideNavBtn" app:fragmentName="Overview" text="{i18n>overView}" id="overview" press="onCustomerNavigationSelect" type="Unstyled"></Button>
					<Button class="sideNavBtn" app:fragmentName="Order" text="{i18n>order}" id="order" press="onCustomerNavigationSelect" type="Unstyled"></Button>
					<Button class="sideNavBtn" app:fragmentName="Wishlist" text="{i18n>wishlist}" id="wishlist" press="onCustomerNavigationWishlistSelect" type="Unstyled"></Button>
					<Button class="sideNavBtn" app:fragmentName="Vouchers" text="{i18n>vouchers}" id="vouchers" press="onCustomerNavigationSelect" type="Unstyled"></Button>
					<Button class="sideNavBtn" app:fragmentName="PersonalDetails" text="{i18n>persDeta}" id="personalDetails" press="onCustomerNavigationSelect" type="Unstyled"></Button>
					<Text class="sideNavText" text="{i18n>more}"></Text>
					<Button class="sideNavBtn" app:fragmentName="HelpFaq" text="{i18n>help}" id="helpFaq" press="onCustomerNavigationSelect" type="Unstyled"></Button>
					<Button class="sideNavBtn" app:fragmentName="ContactUs" text="{i18n>contactUs}" id="contactUs" press="onCustomerNavigationSelect" type="Unstyled"></Button>
				</VBox>
				<Panel id="customerContent">

				</Panel>
			</HBox>
		</content>
		<footer>

		</footer>
	</Page>
</mvc:View>