<mvc:View xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:tnt="sap.tnt"
	xmlns:f="sap.f" controllerName="ag.agasown.controller.Customer">
	<Page id="productDetail" showNavButton="true" navButtonPress="onNavBack">
		<customHeader>
			<core:Fragment fragmentName="ag.agasown.view.fragment.Header" type="XML" />
		</customHeader>
		<content>
			<HBox>
				<VBox>
					<tnt:SideNavigation id="customerNavigation" selectedKey="overview" itemSelect="onCustomerNavigationSelect">
						<tnt:NavigationList>
							<tnt:NavigationListItem text="Eisa Account" icon="sap-icon://employee">
								<tnt:NavigationListItem text="Overview" id="overview" key="Overview"/>
								<tnt:NavigationListItem text="Order" id="order" key="Order"/>
								<tnt:NavigationListItem text="Newsletter" id="newsletter" key="Newsletter" />
								<tnt:NavigationListItem text="Vouchers" id="vouchers" key="Vouchers"/>
								<tnt:NavigationListItem text="Wishlist" id="wishlist" key="Wishlist"/>
								<tnt:NavigationListItem text="Personal Details" id="personalDetails" key="personalDetails"/>
							</tnt:NavigationListItem>
						</tnt:NavigationList>
						<tnt:fixedItem>
							<tnt:NavigationList>
								<tnt:NavigationListItem text="Help FAQ" icon="sap-icon://employee" />
								<tnt:NavigationListItem text="Contact Us" icon="sap-icon://building" />
							</tnt:NavigationList>
						</tnt:fixedItem>
					</tnt:SideNavigation>
				</VBox>
				<VBox id="customerContent">
					
				</VBox>
			</HBox>

		</content>
		<footer>

		</footer>
	</Page>
</mvc:View>