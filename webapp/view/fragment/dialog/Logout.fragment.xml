<core:FragmentDefinition 
xmlns="sap.m" 
xmlns:f="sap.f" 
controllerName="ag.agasown.controller.Registration"
xmlns:core="sap.ui.core">
    <ResponsivePopover showArrow="false" showHeader="false" class="sapUiPopupWithPadding" placement="Bottom">
        <content>
            <VBox class="sapUiSmallMargin go_to_cus_user">
                <Button type="Unstyled" text="{i18n>goToCust}" press=".onNavToCustomer" class="go_to_cus"/>
                <Button type="Unstyled" text="{i18n>logout}" icon="sap-icon://log" press=".onLogout" class="go_to_cus"/>
            </VBox>
        </content>
    </ResponsivePopover>
</core:FragmentDefinition>