<core:FragmentDefinition xmlns="sap.m"
   xmlns:f="sap.f"
   xmlns:core="sap.ui.core">
   <VBox>
      <VBox class="sapUiSmallMargin" >
         <Text class="overviewHeader" text="Your Account" />
         <Text class="overviewText" text="Welcome to private corner of agasown. you can manage your orders, returns and accounts" />
      </VBox>
      <VBox class="sapUiSmallMargin" renderType="Bare">
         <Image class="overviewImage sapUiSmallMarginBottom" src="{path: 'view>/ShoppingBags',
         formatter : '.formatter.pictureUrl'
			}" width="25%"/>
         <Text text="Check our latest product"/>
         <Button width="25%" class="dialogSubmit" text="Continue Shopping" press=".onLoginSubmit" type="Unstyled" />
      </VBox>
   </VBox>
</core:FragmentDefinition>