<core:FragmentDefinition xmlns="sap.m"
   xmlns:f="sap.f"
   xmlns:core="sap.ui.core">
   <VBox>
      <VBox class="sapUiSmallMargin">
         <Text text="Your Account" />
         <Text text="Welcome to private corner of agasown. you can manage your orders, returns and accounts" />
      </VBox>
      <VBox class="sapUiSmallMargin" renderType="Bare" justifyContent="Center" alignItems="Center">
         <Image class="overviewCustomerImage" src="{path: 'view>/ShoppingBags',
         formatter : '.formatter.pictureUrl'
			}" width="25%"/>
         <Button width="25%" class="dialogSubmit" text="Continue Shopping" press=".onLoginSubmit" type="Unstyled" />
      </VBox>
   </VBox>
</core:FragmentDefinition>