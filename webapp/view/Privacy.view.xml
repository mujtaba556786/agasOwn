<mvc:View controllerName="ag.agasown.controller.Information"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="home">
        <customHeader>
            <Bar class="headerBar">
                <contentLeft>
                    <Button class="icon" icon="sap-icon://nav-back" press=".onNavBack"/>
                </contentLeft>
                <contentMiddle>
                    <Image class="header-img" src="{path: 'view>/welcomeLogo', formatter : '.formatter.pictureUrl' }" press="onNavBackHome"></Image>
                </contentMiddle>
                <contentRight>
                </contentRight>
            </Bar>
        </customHeader>
        <content>
            <core:Fragment fragmentName="ag.agasown.view.fragment.information.Privacy" type="XML" />
            <Panel class="fotter">
                <core:Fragment fragmentName="ag.agasown.view.fragment.Footer" type="XML" />
            </Panel>
        </content>
    </Page>
</mvc:View>